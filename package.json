  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@nx/workspace": "17.3.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
