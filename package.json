{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@branch-name-validator-test/root",
  "version": "1.5.111",
  "description": "Branch Name Validator Test",
  "keywords": [
    "github",
    "actions",
    "branch",
    "validation"
  ],
  "homepage": "https://github.com/akikanellis/branch-name-validator-test",
  "bugs": "https://github.com/akikanellis/branch-name-validator-test/issues",
  "license": "MIT",
  "author": "Aki Kanellis <hello@akikanellis.com> (https://akikanellis.com)",
  "repository": "github:akikanellis/branch-name-validator-test",
  "workspaces": [
    "packages/*/*"
  ],
  "scripts": {
    "install:all": "nx run-many --target=install:all --output-style=stream",
    "install:tools": "nx run-many --target=install:tools --output-style=stream",
    "install:dependencies": "nx run-many --target=install:dependencies --output-style=stream",
    "lint": "nx run-many --target=lint --output-style=stream",
    "build": "nx run-many --target=build --output-style=stream",
    "test": "nx run-many --target=test --output-style=stream",
    "test:coverage": "nx run-many --target=test:coverage --output-style=stream",
    "package": "nx run-many --target=package --output-style=stream",
    "clean": "nx run-many --target=clean --output-style=stream"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@nx/workspace": "17.2.8",
    "@tsconfig/strictest": "2.0.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.3",
    "@types/webpack": "5.28.5",
    "jest": "29.7.0",
    "mega-linter-runner": "7.7.0",
    "nx": "17.2.8",
    "rimraf": "5.0.5",
    "terser-webpack-plugin": "5.3.10",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-license-plugin": "4.4.2"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "private": true
}
